<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
android:paddingBottom="@dimen/activity_vertical_margin"
tools:context="work.tarea1.webService.webasignacionHorarios">

<TextView android:text="Direccion Asignacion" android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/textView8"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true" />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Mostrar"
    android:id="@+id/button12"
    android:onClick="WebConsultaAct"
    android:layout_below="@+id/textView8"
    android:layout_marginTop="32dp"
    android:layout_alignParentRight="true"
    android:layout_alignParentEnd="true"
    android:layout_toEndOf="@+id/textView9"
    android:layout_toRightOf="@+id/textView9" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/resultadoWeb"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignRight="@+id/button12"
    android:layout_alignEnd="@+id/button12"
    android:layout_alignParentBottom="true"
    android:layout_below="@+id/button12" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Id Asignacion"
    android:id="@+id/textView9"
    android:layout_above="@+id/resultadoWeb"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignTop="@+id/editText2"
    android:layout_toStartOf="@+id/editText2"
    android:layout_alignRight="@+id/editText2"
    android:layout_alignEnd="@+id/editText2" />

<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/editText2"
    android:layout_alignTop="@+id/button12"
    android:layout_alignRight="@+id/textView8"
    android:layout_alignEnd="@+id/textView8"
    android:layout_marginRight="55dp"
    android:layout_marginEnd="55dp"
    android:layout_alignLeft="@+id/textView8"
    android:layout_alignStart="@+id/textView8"
    android:layout_marginLeft="40dp"
    android:layout_marginStart="40dp" />

</RelativeLayout>
